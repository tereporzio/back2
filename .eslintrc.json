{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "import/no-dynamic-require" : "off",
        "global-require" : "off",
        "no-irregular-whitespace": "off",
        "import/extensions" : "off",
        "camelcase" : "off",
        "radix" : "off",
        "no-undef" : "off",
        "no-await-in-loop" : "off",
        "no-restricted-syntax" : "off",
        "no-plusplus" : "off"

    }
}
